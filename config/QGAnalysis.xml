<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE JobConfiguration PUBLIC "" "JobConfig.dtd" [

    <!ENTITY NEVT "-1">
    <!ENTITY IDIR "/nfs/dust/cms/user/aggleton/CMSSW_8_0_24_patch1/src/UHH2/QGAnalysis/Ntuples">
    <!ENTITY ODIR "/nfs/dust/cms/user/aggleton/CMSSW_8_0_24_patch1/src/UHH2/QGAnalysis/Selection">

    <!-- Z+jets MC -->
    <!ENTITY MC_DYJetsToLL_M-50_HT70to100 "&IDIR;/Ntuple_MC_DYJetsToLL_M-50_HT70to100.root">
    <!ENTITY MC_DYJetsToLL_M-50_HT100to200 "&IDIR;/Ntuple_MC_DYJetsToLL_M-50_HT100to200.root">
    <!ENTITY MC_DYJetsToLL_M-50_HT200to400 "&IDIR;/Ntuple_MC_DYJetsToLL_M-50_HT200to400.root">
    <!ENTITY MC_DYJetsToLL_M-50_HT400to600 "&IDIR;/Ntuple_MC_DYJetsToLL_M-50_HT400to600.root">
    <!ENTITY MC_DYJetsToLL_M-50_HT600to800 "&IDIR;/Ntuple_MC_DYJetsToLL_M-50_HT600to800.root">

    <!-- QCD MC -->
    <!ENTITY MC_QCD_HT100to200 "&IDIR;/Ntuple_MC_QCD_HT100to200.root">
    <!ENTITY MC_QCD_HT200to300 "&IDIR;/Ntuple_MC_QCD_HT200to300.root">
    <!ENTITY MC_QCD_HT300to500 "&IDIR;/Ntuple_MC_QCD_HT300to500.root">
    <!ENTITY MC_QCD_HT500to700 "&IDIR;/Ntuple_MC_QCD_HT500to700.root">
    <!ENTITY MC_QCD_HT700to1000 "&IDIR;/Ntuple_MC_QCD_HT700to1000.root">
]>

<!-- OutputLevel controls which messages are printed; set to VERBOSE or DEBUG for more verbosity, to WARNING or ERROR for less -->
<JobConfiguration JobName="ExampleCycleJob" OutputLevel="INFO">
    <Library Name="libSUHH2QGAnalysis"/>
    <Package Name="SUHH2QGAnalysis.par" />

   <Cycle Name="uhh2::AnalysisModuleRunner" OutputDirectory="&ODIR;/" PostFix="" TargetLumi="1" >

        <InputData Lumi="1" NEventsMax="&NEVT;" Type="MC" Version="MC_DYJetsToLL_M-50_HT100to200" Cacheable="False">
            <In FileName="&MC_DYJetsToLL_M-50_HT100to200;" Lumi="0.0"/>
            <InputTree Name="AnalysisTree" />
        </InputData>

        <InputData Lumi="1" NEventsMax="&NEVT;" Type="MC" Version="MC_DYJetsToLL_M-50_HT200to400" Cacheable="False">
            <In FileName="&MC_DYJetsToLL_M-50_HT200to400;" Lumi="0.0"/>
            <InputTree Name="AnalysisTree" />
        </InputData>

        <InputData Lumi="1" NEventsMax="&NEVT;" Type="MC" Version="MC_DYJetsToLL_M-50_HT400to600" Cacheable="False">
            <In FileName="&MC_DYJetsToLL_M-50_HT400to600;" Lumi="0.0"/>
            <InputTree Name="AnalysisTree" />
        </InputData>

        <InputData Lumi="1" NEventsMax="&NEVT;" Type="MC" Version="MC_DYJetsToLL_M-50_HT600to800" Cacheable="False">
            <In FileName="&MC_DYJetsToLL_M-50_HT600to800;" Lumi="0.0"/>
            <InputTree Name="AnalysisTree" />
        </InputData>

        <InputData Lumi="1" NEventsMax="&NEVT;" Type="MC" Version="MC_QCD_HT100to200" Cacheable="False">
            <In FileName="&MC_QCD_HT100to200;" Lumi="0.0"/>
            <InputTree Name="AnalysisTree" />
        </InputData>

        <InputData Lumi="1" NEventsMax="&NEVT;" Type="MC" Version="MC_QCD_HT200to300" Cacheable="False">
            <In FileName="&MC_QCD_HT200to300;" Lumi="0.0"/>
            <InputTree Name="AnalysisTree" />
        </InputData>

        <InputData Lumi="1" NEventsMax="&NEVT;" Type="MC" Version="MC_QCD_HT300to500" Cacheable="False">
            <In FileName="&MC_QCD_HT300to500;" Lumi="0.0"/>
            <InputTree Name="AnalysisTree" />
        </InputData>

        <InputData Lumi="1" NEventsMax="&NEVT;" Type="MC" Version="MC_QCD_HT500to700" Cacheable="False">
            <In FileName="&MC_QCD_HT500to700;" Lumi="0.0"/>
            <InputTree Name="AnalysisTree" />
        </InputData>

        <InputData Lumi="1" NEventsMax="&NEVT;" Type="MC" Version="MC_QCD_HT700to1000" Cacheable="False">
            <In FileName="&MC_QCD_HT700to1000;" Lumi="0.0"/>
            <InputTree Name="AnalysisTree" />
        </InputData>


        <UserConfig>
            <!-- define which collections to read from the input. Only specify what you need to save I/O time -->
            <Item Name="PrimaryVertexCollection" Value="offlineSlimmedPrimaryVertices" />
            <Item Name="ElectronCollection" Value="slimmedElectronsUSER" />
            <Item Name="MuonCollection" Value="slimmedMuonsUSER" />
            <!-- <Item Name="TauCollection" Value="slimmedTaus" /> -->
            <Item Name="JetCollection" Value="slimmedJets" />
            <Item Name="GenJetCollection" Value="slimmedGenJets" />
            <Item Name="GenParticleCollection" Value="GenParticles" />
            <Item Name="METName" Value="slimmedMETs" />

            <!-- the class name of the AnalysisModule subclasses to run: -->
            <Item Name="AnalysisModule" Value="QGAnalysisModule" />

            <!-- tell AnalysisModuleRunner NOT to use the MC event weight from SFrame; rather let
                 MCLumiWeight (called via CommonModules) calculate the MC event weight. The MC
                 event weight assigned by MCLumiWeight is InputData.Lumi / Cycle.TargetLumi. -->
            <Item Name="use_sframe_weight" Value="false" />

            <Item Name="JetCone" Value="AK4" />
            <Item Name="PURemoval" Value="CHS" />

        </UserConfig>
    </Cycle>
</JobConfiguration>
